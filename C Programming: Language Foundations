#include <stdio.h>
int main(void) {
    int dice1, dice2, sum;
    scanf("%d %d", &dice1, &dice2);
    sum = dice1 + dice2;
    
    if(sum >= 10) {
        printf("Special tax\n36\n");
    } else{
        printf("Regular tax\n%d\n", 2 * sum);
    }
    return 0;
}



#include <stdio.h>
int main(void) {
    int players, weight, team1 = 0, team2 = 0;
    scanf("%d", &players);
    
    for(int i = 0; i < players; i++) {
        scanf("%d", &weight);
        team1 += weight;
        
        scanf("%d", &weight);
        team2 += weight;
    }
    
    if(team1 > team2) {
        printf("Team 1 has an advantage\n");
    } else {
        printf("Team 2 has an advantage\n");
    }
    printf("Total weight for team 1: %d\n", team1);
    printf("Total weight for team 2: %d\n", team2);
    
    return 0;
}



#include <stdio.h>
int main() {
    int age, lagWeight, price;
    scanf("%d", &age);
    scanf("%d", &lagWeight);
    
    if (age == 60) {
        price = 0;
    } else if(age < 10){
        price = 5;
    } else{ 
        price = 30;
        if (lagWeight > 20) {
            price += 10;
        }
    }
    printf("%d\n", price);
    return 0;
}



#include <stdio.h>
int main() {
    int height, leaflets;
    scanf("%d\n %d", &height, &leaflets);
    
    if(height <= 5 && leaflets >= 8){
        printf("Tinuviel");
    } else if(height >= 10 && leaflets >= 10){
        printf("Calaelen");
    } else if(height <= 8 && leaflets <= 5){
        printf("Falarion");
    } else if(height >= 12 && leaflets <= 7){
        printf("Dorthonion");
    } else{
        printf("Uncertain");
    }
    return 0;
}



#include <stdio.h>
int main() {
    int ingridients[10];
    int id;
    int i;
    
    for(i = 0; i < 10; i++) {
        scanf("%d", &ingridients[i]);
    }
    scanf("%d", &id);
    printf("%d\n", ingridients[id]);
    return 0;
}



