#include <stdio.h>
int main(void){
    int i = 0;
    for(i=0; i<3; i++){
        printf("C is fun!\n");
    }
    return 0;
}



#include <stdio.h>
int main(void){
    int i = 0;
    for(i = 0; i < 3; i++) {
        printf("C is fun!\n");
    }
    printf("\n");
    for(i = 0; i < 5; i++) {
        printf("We can do everything with it!\n");
    }
    return 0;
}



#include <stdio.h>
int main(void) {
    
    int i;
    
    printf("+");
    for (i = 0; i < 23; i++) {
        printf("-");
    }
    printf("+\n");

    for (i = 0; i < 3; i++) {
        printf("| o | X | o | X | o | X |");
        printf("\n");
        printf("| X | o | X | o | X | o |");
        printf("\n");
    }
    printf("+");
    for (i = 0; i < 23; i++) {
        printf("-");
    }
    printf("+");

    return(0);
}



